{
  "name": "edb-monorepo",
  "version": "0.0.1",
  "description": "Ethereum Debugger - Monorepo for Rust backend and TypeScript frontends",
  "private": true,
  "scripts": {
    "build": "pnpm run -r build",
    "test": "pnpm run -r test",
    "lint": "pnpm run -r lint",
    "dev": "pnpm run -r dev",
    "type-check": "pnpm run -r type-check",
    "clean": "pnpm run -r clean && rm -rf node_modules",
    "build:rust": "cargo build --workspace",
    "test:rust": "cargo test --workspace",
    "clippy": "cargo clippy --all-targets --all-features -- -D warnings",
    "fmt": "cargo fmt --all"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@10.5.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/edb-rs/edb"
  },
  "keywords": [
    "ethereum",
    "debugger",
    "solidity",
    "evm",
    "blockchain",
    "typescript",
    "rust"
  ],
  "authors": [
    "Zhuo Zhang",
    "Wuqi Zhang"
  ],
  "license": "AGPL-3.0"
}