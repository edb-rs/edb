{
  "name": "edb-typescript-workspace",
  "version": "0.0.1",
  "description": "TypeScript workspace for EDB frontend clients",
  "private": true,
  "scripts": {
    "build": "pnpm run -r build",
    "dev": "pnpm run -r dev",
    "test": "pnpm run -r test",
    "lint": "pnpm run -r lint",
    "type-check": "pnpm run -r type-check",
    "clean": "pnpm run -r clean && rm -rf node_modules packages/*/node_modules packages/*/dist"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  }
}